{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "A custom schema for Live2D Moc3 drawables rendering",
  "type": "object",
  "properties": {
    "model": {
      "type": "string"
    },
    "order": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "hidden": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "model",
    "order",
    "hidden"
  ]
}
